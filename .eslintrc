{
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules": {
        "arrow-parens": "off",
        "max-len": "off",
        "indent": "off",
        "linebreak-style": "off",
        "object-curly-spacing": "off",
        "comma-dangle": "off",
        "keyword-spacing": "off",
        "eol-last": "off",
        "require-jsdoc": "off"

    },
    "extends": [
        "eslint:recommended",
        "google"
    ]
}